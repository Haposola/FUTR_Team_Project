<h2>User Management</h2>
<%= link_to 'Applications','/admins/usrmgmt/applications' %>
<%= link_to 'Our Big Users','/admins/usrmgmt/uncommon'%>
<table>
	<tr>
		<th>Email</th>
		<th>Nickname</th>
		<th>Name</th>
		<th>Nation</th>
		<th>Category</th>
		<th>Real Name Confirmation</th>
		<th>Last Login Time</th>
	</tr>
<% @users.each do |user| %>
	<tr>
		<td><%= user.email%></td>
		<td><%= user.nickname%></td>
		<td><%= user.name%></td>
		<td><%= user.nation%></td>
		<td><%= user.category%></td>
		<td><%= user.realconfirm%></td>
		<td><%= user.last_sign_in_at%></td>
	</tr>
<%end%>
</table>